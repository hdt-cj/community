<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nowcoder.community.dao.StudentsMapper">

    <select id="selectByidcard" resultType="Students">
        select id,idcard,name,age,xueyuan
        from students
        where idcard = #{idcard}
    </select>
</mapper>